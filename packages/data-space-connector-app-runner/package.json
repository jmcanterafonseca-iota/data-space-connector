{
	"name": "@twin.org/data-space-connector-app-runner",
	"version": "0.0.1-next.1",
	"description": "Data Space Connector App Runner",
	"repository": {
		"type": "git",
		"url": "https://github.com/twinfoundation/data-space-connector.git",
		"directory": "packages/data-space-connector-app-runner"
	},
	"author": "jose.cantera@iota.org",
	"license": "Apache-2.0",
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"clean": "rimraf dist coverage docs/reference",
		"build": "tsc",
		"bundle:esm": "rollup --config rollup.config.mjs --environment MODULE:esm",
		"bundle:cjs": "rollup --config rollup.config.mjs --environment MODULE:cjs",
		"bundle": "npm run bundle:esm && npm run bundle:cjs",
		"dist": "npm run clean && npm run build && npm run bundle",
		"prepare": "ts-patch install -s"
	},
	"dependencies": {
		"@twin.org/data-space-connector-models": "0.0.1-next.1",
		"@twin.org/logging-models": "next",
		"@twin.org/engine-models": "next",
		"@twin.org/engine-core": "next",
		"@twin.org/core": "next",
		"@twin.org/nameof": "next"
	},
	"devDependencies": {
		"@twin.org/nameof-transformer": "next",
		"@twin.org/nameof-vitest-plugin": "next",
		"@types/node": "22.14.1",
		"copyfiles": "2.4.1",
		"rimraf": "6.0.1",
		"rollup": "4.40.0",
		"ts-patch": "3.3.0",
		"typescript": "5.8.3"
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.mjs",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs"
		},
		"./locales/*.json": "./locales/*.json"
	},
	"files": [
		"dist/cjs",
		"dist/esm",
		"dist/types",
		"locales",
		"docs"
	]
}
